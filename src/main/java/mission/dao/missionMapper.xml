<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 <mapper namespace="missionSQL">
 
 	<insert id="write" parameterType="mission.bean.MissionDTO">
 		insert into mission values(seq_ohmygoal.NEXTVAL, 
 									, #{url}
 									, #{img}
 									, #{subject}
 									, #{category}
 									, #{id}
 									, #{startDate}
 									, #{endDate}
 									, #{membernum}
 									, #{content}
 									, #{members})
 	</insert>
 </mapper>